package bitMast;

import java.util.Scanner;

public class No11723 {

	public static void main(String[] args) {
		@SuppressWarnings("resource")
		Scanner sc = new Scanner(System.in);
		int cnt = sc.nextInt();
		int s = 0;
		int num =0;
		while(cnt-->0){
			String line = sc.next();
			switch(line) {
			case "add" :
				num = sc.nextInt();num--;
				s = s | (1<<num);
				break;
			case "remove" :
				num = sc.nextInt();num--;
				s = s & ~(1<<num);
				break;
			case "check":
				num = sc.nextInt();num--;
				int res =s&(1<<num); 
				if(res==0) {
					System.out.println("1");
				} else {
					System.out.println("0");
				}
				break;
			case "toggle":
				num = sc.nextInt();num--;
				s = s^(1<<num);
				break;
			case "empty":
				s = 0;
				break;
			case "all" :
				num = sc.nextInt();num--;
				s = (1<<num)-1;
				break;
			default:
				break;		
			}	
		}
	}
}
