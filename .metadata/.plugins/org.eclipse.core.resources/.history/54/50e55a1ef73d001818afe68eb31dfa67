package swExpertAcademy;

import java.io.BufferedReader;
import java.io.BufferedWriter;
import java.io.IOException;
import java.io.InputStreamReader;
import java.io.OutputStreamWriter;
import java.util.HashMap;

/**
 * @author ginTonic
 * @Title 활주로건설
 * 2018. 4. 12.
 */
public class No4014 {

	/**
	 * @param args
	 * @throws IOException 
	 * @throws NumberFormatException 
	 */
	static int n;
	static int x;
	public static void main(String[] args) throws NumberFormatException, IOException {
		BufferedReader br = new BufferedReader(new InputStreamReader(System.in));
		BufferedWriter bw = new BufferedWriter(new OutputStreamWriter(System.out));
		int T = Integer.valueOf(br.readLine());
		
		while(T-->0) {
			String[] NX = br.readLine().split(" ");
			n = Integer.valueOf(NX[0]); x = Integer.valueOf(NX[1]);
			int[][] map = new int[n][n];
			for(int i=0; i<n; i++) {
				String[] line = br.readLine().split(" ");
				for(int j=0; j<n; j++) {
					map[i][j] = Integer.valueOf(line[j]);
				}
			}
			for(int i=0; i<n; i++) {
				System.out.println(checkCol(map, i));	
			}
			
			
		}
	}
	private static void solve(int[][] map) {
		
	}
	//가로방향
	private static int checkCol(int[][] map, int row) {
		int w = 1;
		boolean flag = false;
		for(int i=0; i<n-1; i++) {
			if(map[row][i]==map[row][i+1]) w+=1;
			else {
				int diff = Math.abs(map[row][i]-map[row][i+1]);
				if(diff>1) return 0;
				if(flag) {
					if(w<x) return 0;
				}
				if(map[row][i]<map[row][i+1]) {
					if(w<x) return 0;
				}
				else {
					flag = true;
				}
				w=1;
			}
		}
		return 1;
	}
	//세로방향
	private static void checkRow(int[][] map, int col) {
		
	}

}
