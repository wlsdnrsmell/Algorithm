package bitMast;

/*
 * @Author : wookTire
 * 
 * @Title : 비트논리연산자
 */
public class BitWiseOper {

	public static void main(String[] args) {
		int x = 10;
		int y = 20;
		System.out.printf("x = %#b %s%n",x,Integer.toBinaryString(x));
		System.out.printf("y = %#b %s%n",y,Integer.toBinaryString(y));
	}

}
