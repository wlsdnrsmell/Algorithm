package etc;

import java.io.BufferedReader;
import java.io.IOException;
import java.io.InputStreamReader;
import java.util.StringTokenizer;

/**
 * @author ginTonic
 * @Title 
 * 2018. 4. 13.
 */
public class No2602 {

	/**
	 * @param args
	 * @throws IOException 
	 */
	public static void main(String[] args) throws IOException {
		BufferedReader br = new BufferedReader(new InputStreamReader(System.in));
		String ansString = br.readLine();
		StringTokenizer st = new StringTokenizer(ansString);
		String[] str = new String[ansString.length()];
		int idx = 0;
		while(st.hasMoreTokens())
			str[idx] = st.nextToken();
		
		String devil = br.readLine();
		int n = devil.length();
		String[] arrDevil = new String[n];
		idx = 0;
		while(st.hasMoreTokens())
			arrDevil[idx] = st.nextToken();
		String angel = br.readLine();
		String[] arrAngel = new String[n];
		idx = 0;
		while(st.hasMoreTokens())
			arrAngel[idx] = st.nextToken();
		
	}

}
